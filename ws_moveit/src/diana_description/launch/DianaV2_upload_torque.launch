<?xml version="1.0"?>
<launch>
  <arg name="limited" default="false" doc="If true, limits joint range." />
  
  <param unless="$(arg limited)" name="robot_description" command="$(find xacro)/xacro --inorder '$(find diana_description)/urdf/DianaV2_joint_torque_robot.urdf.xacro'" />
  <param if="$(arg limited)" name="robot_description" command="$(find xacro)/xacro --inorder '$(find diana_description)/urdf/DianaV2_joint_limited_torque_robot.urdf.xacro'" />
</launch>
